                            <div class="modal fade" id="gameModal_150_9" tabindex="-1" role="dialog" aria-labelledby="gameModalTitle_150_9" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document" style="max-width: 500px;">
        <div class="modal-content p-3">
            <div class="modal-header">
                <h5 class="modal-title" id="gameModalTitle_150_9">Play the Game to get the Rewards not get</h5>
                <button type="button" class="close close-confirm-btn" id="closeBtn_gameModal_150_9" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body p-0 d-flex justify-content-center align-items-center" style="height: 500px;">
                                    <div class="click-the-circle-game-component">
    <div class="align-items-center mb-2">
        <h5 class="mb-0"> Click the circle as quickly as you can</h5>
        <div class="mb-2 description">
            Click as many circles as possible in given time. Each click counts as one point.
        </div>

        <div class="d-flex justify-content-between mb-2">
            <div>Score: <strong id="clickGameScore">0</strong></div>
            <div>Time Left: <strong id="clickGameTimer">90</strong>s</div>
        </div>

        <div class="text-center mb-3">
            <button id="startClickGame" class="btn btn-primary">Start Game</button>
        </div>
    </div>

    <div class="click-the-circle-area border rounded position-relative" >
        <div class="click-the-circle-target" id="clickTheCircleTarget" ></div>
    </div>

    <div class="mt-3 text-center fw-bold text-primary" id="clickGameResult"></div>

    <style>
        .click-the-circle-game-component .click-the-circle-area {
            display: none;
            background-color: #f8f9fa;
            overflow: hidden;
            height: 350px;
            width: 450px;
        }

        .click-the-circle-game-component .click-the-circle-target {
            background-color: #007bff;
            border-radius: 50%;
            position: absolute;
            cursor: pointer;
            transition: top 0.2s, left 0.2s, width 0.2s, height 0.2s;
        }
    </style>

    <script>
        var rewardDetail = {"id":9,"feature":"Log your Water Intake","feature_key":"log-water-intake","point":"","is_gamification":1,"description":"\u003Cp\u003ETesting\u003C\/p\u003E","status":"active","order":2,"created_by":1,"updated_by":1,"created_at":"2025-07-17T19:46:36.000000Z","updated_at":"2025-07-27T11:34:18.000000Z","reward_game":{"id":25,"reward_management_id":9,"game_type":"specific","game_key":"click-the-circle","min_points":2,"max_points":8,"duration":90,"score":50,"created_by":1,"updated_by":1,"created_at":"2025-07-27T11:16:54.000000Z","updated_at":"2025-07-27T11:34:18.000000Z","game":{"id":2,"title":"Click the circle as quickly as you can","description":"Click as many circles as possible in given time. Each click counts as one point.","game_key":"click-the-circle","status":"active","created_by":1,"updated_by":1,"created_at":"2025-07-18T01:15:28.000000Z","updated_at":"2025-07-18T01:15:28.000000Z"}}};
        document.addEventListener("DOMContentLoaded", function () {
            const wrapper = document.querySelector(".click-the-circle-game-component");
            if (!wrapper) return;

            const area = wrapper.querySelector(".click-the-circle-area");
            const target = wrapper.querySelector("#clickTheCircleTarget");
            const scoreDisplay = wrapper.querySelector("#clickGameScore");
            const timerDisplay = wrapper.querySelector("#clickGameTimer");
            const resultDisplay = wrapper.querySelector("#clickGameResult");
            const startButton = wrapper.querySelector("#startClickGame");

            let score = 0;
            let timeLeft = rewardDetail.reward_game.duration;
            let timer;
            let moveDelay;
            let isPlaying = false;

            function randomPositionAndSize() {
                const size = Math.floor(Math.random() * 20) + 40; // 40px to 60px
                const maxLeft = area.clientWidth - size;
                const maxTop = area.clientHeight - size;

                return {
                    left: Math.floor(Math.random() * maxLeft),
                    top: Math.floor(Math.random() * maxTop),
                    size: size
                };
            }

            function showTarget() {
                const pos = randomPositionAndSize();
                target.style.left = `${pos.left}px`;
                target.style.top = `${pos.top}px`;
                target.style.width = `${pos.size}px`;
                target.style.height = `${pos.size}px`;
                target.style.display = "block";
            }

            function startGame() {
                const wrapper = document.querySelector(".click-the-circle-game-component");
                wrapper.querySelector(".click-the-circle-area").style.display = 'block';
                wrapper.querySelector(".description").style.display = 'none';
                startButton.style.display = 'none';
                if (isPlaying) return;

                isPlaying = true;
                score = 0;
                timeLeft = rewardDetail.reward_game.duration;
                scoreDisplay.textContent = score;
                timerDisplay.textContent = timeLeft;
                resultDisplay.textContent = "";
                startButton.disabled = true;

                showTarget();

                timer = setInterval(() => {
                    timeLeft--;
                    timerDisplay.textContent = timeLeft;
                    if (timeLeft <= 0) {
                        endGame();
                    }
                }, 1000);
            }

            function endGame() {
                const wrapper = document.querySelector(".click-the-circle-game-component");
                wrapper.querySelector(".click-the-circle-area").style.display = 'none';
                wrapper.querySelector(".description").style.display = 'block';


                isPlaying = false;
                clearInterval(timer);
                clearTimeout(moveDelay);
                target.style.display = "none";
                startButton.disabled = false;
                resultDisplay.textContent = `Game Over! You scored ${score} points.`;

                if(score){
                    $.ajax({
                        type: "POST",
                        url: "http://127.0.0.1:8000/common/reward-management/save-reward",
                        data: {
                            _token: "eqC98mAfzyr1EloJuvNBeP0CiUQv69taxUKzDVdn",
                            feature_key: "log-water-intake",
                            module_key: "water-tracker",
                            module_id: "",
                            video_id : "",
                            game_key : 'click-the-circle',
                            score : score,
                        },
                        success: function(response) {
                            $('#reviewPointsBtn').prop('disabled', false);
                            if (response.success) {
                                _toast.customSuccess(response.message);
                                setTimeout(function() {
                                    window.location.reload();
                                }, 3000)
                            } else {
                                _toast.error('Somthing went wrong. please try again');
                            }
                        },
                        error: function(err) {
                            $('#reviewPointsBtn').prop('disabled', false);
                            _toast.error('Please try again.');
                        },
                    });
                }
            }

            target.addEventListener("click", () => {
                if (!isPlaying) return;
                score++;
                scoreDisplay.textContent = score;
                target.style.display = "none";
                // Faster appearance (after 150–250ms)
                const delay = Math.floor(Math.random() * 100) + 150;
                moveDelay = setTimeout(showTarget, delay);
            });

            startButton.addEventListener("click", startGame);
        });
    </script>
</div>
                            </div>
        </div>
    </div>
</div>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
(function () {
    const modalId = "#gameModal_150_9";
    const closeBtnId = "#closeBtn_gameModal_150_9";
    let isClosingConfirmed = false;

    const modal = $(modalId);

    

    function confirmModalClose_150_9() {
        bootbox.confirm({
            title: "Are you sure you want to close the game?",
            message: "Closing now may forfeit your chance to earn rewards.",
            buttons: {
                confirm: {
                    label: 'Yes, Close',
                    className: 'btn-danger'
                },
                cancel: {
                    label: 'No, Stay',
                    className: 'btn-secondary'
                }
            },
            callback: function(result) {
                if (result) {
                    $.ajax({
                        type: "POST",
                        url: "http://127.0.0.1:8000/common/reward-management/save-reward",
                        data: {
                            _token: "eqC98mAfzyr1EloJuvNBeP0CiUQv69taxUKzDVdn",
                            feature_key: "log-water-intake",
                            module_id: "",
                            game_key : "click-the-circle",
                            result : 'not_played',
                        },
                        success: function(response) {
                            $('#reviewPointsBtn').prop('disabled', false);
                            if (response.success) {
                                _toast.customSuccess(response.message);
                                setTimeout(function() {
                                    window.location.reload();
                                }, 3000)
                            } else {
                                _toast.error('Somthing went wrong. please try again');
                            }
                        },
                        error: function(err) {
                            $('#reviewPointsBtn').prop('disabled', false);
                            _toast.error('Please try again.');
                        },
                    });
                }
            }
        });
    }

    $(document).ready(function () {
        $(closeBtnId).on('click', function () {
            confirmModalClose_150_9();
        });

        modal.on('hide.bs.modal', function (e) {
            if (!isClosingConfirmed) {
                e.preventDefault();
                confirmModalClose_150_9();
                return false;
            } else {
                isClosingConfirmed = false;
            }
        });

        $(modalId).modal({
            backdrop: 'static',
            keyboard: false
        });
    });
    
   
})();
</script>
            

        </section>

        
